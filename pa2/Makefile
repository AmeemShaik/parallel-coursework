gcc:

	gcc -O3 quicksort.c -o quicksort_gcc -lm

	gcc -O3 -DPRINTMODE quicksort.c -o quicksortPrint_gcc -lm

	gcc -O3 quicksort_cilk.c -o quicksort_cilk_gcc -lcilkrts -ldl -lm -fcilkplus

	gcc -O3 -DPRINTMODE quicksort_cilk.c -o quicksort_cilk_print_gcc -lcilkrts -ldl -lm -fcilkplus

icc:

	icc -O3 quicksort.c -o quicksort_icc

	icc -O3 -DPRINTMODE quicksort.c -o quicksortPrint_icc

	icc -O3 quicksort_cilk.c -o quicksort_cilk_icc

	icc -O3 -DPRINTMODE quicksort_cilk.c -o quicksort_cilk_print_icc
